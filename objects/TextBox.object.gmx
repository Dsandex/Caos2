        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Draws text in a text box effect.
Dialogs_Features(game_intro);

if(str &lt; string_length(ds_list_find_value(text, t))){
    str += vel; // Speed of text.
}

if(str &gt;= string_length(ds_list_find_value(text, t))){
    draw_skip = true;   
    if((keyboard_check_pressed(ord('Z')) or mouse_check_button(mb_left)) and can_continue){
        t++;
        str = 0;
        draw_skip = false;
        if(ds_list_size(text) &lt;= t){
            instance_destroy();
            with (Player) {can_move = true;}
        }
    }
}else{
    draw_skip = false;
    if((keyboard_check(ord('Z')) or mouse_check_button(mb_left)) and (str &lt; string_length(ds_list_find_value(text, t)))){
        str += (vel + 1);
    }
}

